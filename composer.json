{
    "name": "bigbit/di-meta",
    "description": "Dependency Inject meta cache",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrzej Heczko",
            "email": "andrzej.heczko@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BigBIT\\DIMeta\\": "src/"
        }
    },
    "require": {
        "psr/simple-cache": "^1.0",
        "php": "^7.2"
    },
    "require-dev": {
        "symfony/cache": "^4.0",
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.11"
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml",
        "stan": "phpstan analyse --level 7 -c phpstan.neon src"
    }
}
